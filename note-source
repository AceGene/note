alias note-open="nohup gedit ~/gene-ws/note/note ~/gene-ws/note/note-init ~/gene-ws/note/note-source /usr/share/bash-completion/completions/notes > /dev/null 2>&1 &"
#### The following takes the underlying notes 
if [ -f /usr/share/bash-completion/completions/notes ]; then
. /usr/share/bash-completion/completions/notes
else
	echo Completion command for notes cmd was not found
fi
complete -o filenames -o nospace -F _notes note # -o dirnames
